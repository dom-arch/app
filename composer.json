{
  "name": "dom-arch/app",
  "description": "The DOMArch app context",
  "license": "MIT",
  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/dom-arch/php-dom"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dom-arch/dom-arch"
    },
    {
      "type": "vcs",
      "url": "https://github.com/dom-arch/indoctrinated"
    }
  ],
  "autoload": {
    "classmap": ["classes/Entities"],
    "psr-4": {
      "Assemblies\\": "classes/Assemblies",
      "Lib\\": "classes/Lib",
      "Modules\\": "classes/Modules",
      "Providers\\": "classes/Providers",
      "Repositories\\": "classes/Repositories"
    }
  },
  "require": {
    "php": ">=7.0.1",
    "dom-arch/dom-arch": ">=0.7.0",
    "dom-arch/php-dom": ">=0.9",
    "dom-arch/indoctrinated": ">=0.9.8"
  }
}
